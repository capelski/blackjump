{
  "name": "blackjump",
  "version": "1.18.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "npx eas-cli@latest build --platform android",
    "build:android:apk": "npx eas-cli@latest build --platform android --profile preview",
